table 'item'
    - id int(50)
    - nama varchar(50)
    - harga float

table 'user'
    - id serial
    - username varchar(20)
    - password int(30)
    - role varchar(10)

tble 'activity_log'
    - id_act serial
    - id_user int
    - date string
    - username string
    - tipe_act string